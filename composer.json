{
  "repositories": [
    {
      "type": "composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": ">=4.8",
    "wpackagist-theme/twentyseventeen": "1.3"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "plugins/{$name}/": ["type:wordpress-plugin"],
        "content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}